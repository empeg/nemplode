<UserControl x:Class="NEmplode.ResultPane"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:NEmplode="clr-namespace:NEmplode" mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <NEmplode:ResultPaneContentTemplateSelector x:Key="contentTemplateSelector" />
    </UserControl.Resources>

    <ContentControl
        Content="{Binding Path=Content}"
        ContentTemplateSelector="{StaticResource contentTemplateSelector}"
        />
</UserControl>
